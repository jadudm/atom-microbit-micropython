{
  "name": "microbit-micropython",
  "main": "./lib/main",
  "version": "0.3.0",
  "description": "Microbit Micropython support for Atom",
  "keywords": [],
  "repository": "https://github.com/wendlers/atom-microbit-micropython",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-package-deps": "^4.6.0",
    "serialport": "^4.0.7"
  },
  "package-deps": [
    "termination",
    "console-panel",
    "tool-bar",
    "busy"
  ],
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "^0 || ^1": "consumeToolBar"
      }
    },
    "console-panel": {
      "versions": {
        "^1.0.0": "consumeConsolePanel"
      }
    },
    "runInTerminal": {
      "versions": {
        "^0.14.5": "consumeRunInTerminal"
      }
    },
    "busy": {
      "versions": {
        "^1.0.0": "consumeBusy"
      }
    }
  }
}
